{
  "$schema": "https://moonlight-mod.github.io/manifest.schema.json",
  "id": "decor",
  "version": "1.1.2",
  "apiLevel": 2,
  "meta": {
    "name": "Decor",
    "tagline": "Custom avatar decorations",
    "description": "Requires interaction with the Decor CDN.",
    "authors": ["NotNite", "Cynosphere", "FieryFlames"],
    "tags": ["profiles"],
    "source": "https://github.com/NotNite/my-moonlight-extensions"
  },
  "dependencies": ["spacepack", "common"],
  "settings": {
    "baseUrl": {
      "description": "Base URL",
      "type": "string",
      "default": "https://decor.fieryflames.dev/"
    },
    "cdnUrl": {
      "description": "CDN URL",
      "type": "string",
      "default": "https://decorcdn.fieryflames.dev/"
    },
    "appId": {
      "description": "Application ID",
      "type": "string",
      "default": "1096966363416899624"
    }
  }
}
